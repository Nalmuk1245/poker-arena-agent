{
  "name": "poker-arena-agent",
  "version": "1.0.0",
  "description": "AI-powered Texas Hold'em poker agent for Monad blockchain Gaming Arena",
  "main": "agent/src/index.ts",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "deploy:testnet": "npx hardhat run scripts/deploy.ts --network monadTestnet",
    "deploy:local": "npx hardhat run scripts/deploy.ts --network hardhat",
    "agent": "npx ts-node agent/src/index.ts",
    "simulate": "npx ts-node scripts/simulateGames.ts",
    "match": "npx ts-node scripts/runMatch.ts",
    "mcp:build": "cd mcp && npx tsc --project tsconfig.json",
    "mcp:start": "node mcp/build/mcp/src/index.js"
  },
  "keywords": [
    "poker",
    "ai",
    "monad",
    "blockchain",
    "gaming-arena",
    "openclaw"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@types/node": "^25.2.0",
    "hardhat": "^2.28.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@openzeppelin/contracts": "^5.4.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "pokersolver": "^2.1.4",
    "viem": "^2.45.1",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  }
}
